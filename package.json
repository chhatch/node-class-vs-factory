{
  "name": "function-vs-class-test",
  "version": "1.0.0",
  "private": true,
  "description": "Compare memory usage of class vs factory function instances using heap snapshots.",
  "main": "dist/main.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "start:class": "node --expose-gc dist/main.js --approach class --count 100000",
    "start:factory": "node --expose-gc dist/main.js --approach factory --count 100000",
    "start:factory-thin": "node --expose-gc dist/main.js --approach factory-thin --count 100000",
    "start:factory-prototype": "node --expose-gc dist/main.js --approach factory-prototype --count 100000"
  },
  "keywords": [
    "typescript",
    "node",
    "memory",
    "heap",
    "class",
    "factory"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.30",
    "rimraf": "^6.0.0",
    "typescript": "^5.6.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
